<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcase.tao.user.vo.User" table="user">
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="username" column="username" type="string"/>
		<property name="password" column="password" type="string" />
		<property name="phonenumber" column="phonenumber" type="string" />
		<property name="email"  column="email" type="string"/>
		<property name="studentid" column="studentid" type="string" />
		<property name="grade" column="grade" type="string" />
		<property name="classes"  column="classes" type="string"/>
		<property name="dormitory"  column="dormitory" type="string"/>
	
	</class>
</hibernate-mapping>
